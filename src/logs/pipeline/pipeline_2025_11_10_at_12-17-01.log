ELLOW))\n            self.wait(0.5)\n\n        # Hold the final scene\n        self.wait(2)'}, {'script_seq2': 'from manim import *\n\nclass LabelPlacementScene(Scene):\n    def construct(self):\n        # Start with the square and labels from Scene 1\n        square1 = Square(side_length=3, color=BLUE, fill_opacity=0.1).shift(LEFT*2)\n        labels1 = VGroup(\n            MathTex("A").move_to(square1.get_corner(UP+LEFT)),\n            MathTex("B").move_to(square1.get_corner(UP+RIGHT)),\n            MathTex("C").move_to(square1.get_corner(DOWN+RIGHT)),\n            MathTex("D").move_to(square1.get_corner(DOWN+LEFT))\n        )\n        self.play(DrawBorderThenFill(square1), Write(labels1))\n        self.wait(1)\n\n        # Animate a second square appearing to the right, slightly smaller and with different colored labels\n        square2 = Square(side_length=2.5, color=ORANGE, fill_opacity=0.1).shift(RIGHT*2)\n        labels2 = VGroup(\n            MathTex("W").move_to(square2.get_corner(UP+LEFT) + LEFT*0.2 + DOWN*0.2),\n            MathTex("X").move_to(square2.get_corner(UP+RIGHT) + RIGHT*0.2 + DOWN*0.2),\n            MathTex("Y").move_to(square2.get_corner(DOWN+RIGHT) + LEFT*0.2 + UP*0.2),\n            MathTex("Z").move_to(square2.get_corner(DOWN+LEFT) + RIGHT*0.2 + UP*0.2)\n        )\n        self.play(DrawBorderThenFill(square2), Write(labels2))\n        self.wait(1)\n\n        # Show the labels on the second square initially misaligned, overlapping the sides\n        self.play(Indicate(labels2), run_time=2)\n        self.wait(1)\n\n        # Move the misaligned labels to their correct corner positions, one by one\n        correct_positions = [square2.get_corner(corner) for corner in [UP+LEFT, UP+RIGHT, DOWN+RIGHT, DOWN+LEFT]]\n        for label, pos in zip(labels2, correct_positions):\n            self.play(label.animate.move_to(pos))\n            self.wait(0.5)\n\n        # Compare both squares side by side, highlighting the correctly labeled square with a green checkmark\n        checkmark = SVGMobject("checkmark.svg").scale(0.5).move_to(square1.get_center() + UP*1.5).set_color(GREEN)\n        self.play(FadeIn(checkmark))\n        self.wait(2)\n\n        # Fade out the incorrectly labeled square, leaving only the correct example\n        self.play(FadeOut(square2), FadeOut(labels2), FadeOut(checkmark))\n        self.wait(1)'}, {'script_seq3': 'from manim import *\n\nclass ClassroomAnimation(Scene):\n    def construct(self):\n        # Start with a blank canvas\n        self.camera.background_color = WHITE\n\n        # Fade in a diagram of a classroom with a square table in the center\n        classroom = Rectangle(width=12, height=8, color=LIGHT_GRAY, fill_opacity=0.5)\n        table = Square(side_length=3, color=DARK_BROWN).move_to(ORIGIN)\n        self.play(FadeIn(classroom), FadeIn(table))\n\n        # Animate four students appearing, each sitting at a corner of the table\n        students = VGroup(\n            Dot(table.get_corner(UL) + LEFT * 0.5 + UP * 0.5, radius=0.2, color=BLUE),\n            Dot(table.get_corner(UR) + RIGHT * 0.5 + UP * 0.5, radius=0.2, color=GREEN),\n            Dot(table.get_corner(DL) + LEFT * 0.5 + DOWN * 0.5, radius=0.2, color=YELLOW),\n            Dot(table.get_corner(DR) + RIGHT * 0.5 + DOWN * 0.5, radius=0.2, color=RED)\n        )\n        self.play(FadeIn(students))\n\n        # Add labels above each student‚Äôs head with their names\n        labels = VGroup(\n            Text("Alice", font_size=24).next_to(students[0], UP, buff=0.5),\n            Text("Bob", font_size=24).next_to(students[1], UP, buff=0.5),\n            Text("Charlie", font_size=24).next_to(students[2], DOWN, buff=0.5),\n            Text("Diana", font_size=24).next_to(students[3], DOWN, buff=0.5)\n        )\n        self.play(FadeIn(labels))\n\n        # Move the labels to the corners of the table, aligning them perfectly\n        label_positions = [table.get_corner(UL) + UP * 0.5, table.get_corner(UR) + UP * 0.5, \